    "keyword.operator.new": "#FFD580",

    "method.declaration.async": "#FFD580",
    "method.defaultLibrary": "#F27983",
    "method": "#FFD580",
    "number": { "foreground": "#8C9EFF", "fontStyle": "" },
    "newOperator": "#C586C0",
    "numberLiteral": "#b5cea8",
    // "property.declaration": "#BAE67E",
    "readonly.local": "#BAE67E",
    "stringLiteral": "#ce9178",
    "support.defaultLibrary": "#F27983",

    "type": { "foreground": "#B9F6CA", "fontStyle": "" },
    "interface": { "foreground": "#B9F6CA", "fontStyle": "" },
    // "interface": { "foreground": "#BEE2E1DD", "fontStyle": "" },

    "property": { "foreground": "#CBCCC6", "fontStyle": "" },
    "property.declaration": { "foreground": "#BAE67E", "fontStyle": "" },
    "parameter": { "foreground": "#CBCCC6", "fontStyle": "" },
    "parameter.declaration": { "foreground": "#D4BFFF", "fontStyle": "" },

    // "typeParameter": "#ff0000",

    "module": { "foreground": "#F29E74", "fontStyle": "" },
    "variable": "#CBCCC6",
    "variable.defaultLibrary": "#59C9DE99",
    "variable.local": "#CBCCC6",
    "variable.language.this": { "foreground": "#5CCFE6", "fontStyle": "italic" }
  },

  "tokenColors": [
    // -------------- ðŸ”¥ UNKNOWN -----------------

    {
      "scope": ["source meta.class meta.field.declaration meta.type.annotation keyword.operator"],
      "settings": {
        "foreground": "#CBCCC6B3",
        "fontStyle": ""
      }
    },
    // -------------- ðŸ”¥ TS -----------------
    {
      "scope": [
        "source.ts meta.interface meta.field.declaration meta.type.annotation keyword.operator.type.annotation",
        "source meta.type meta.object.type keyword.operator"
      ],
      "settings": {
        "foreground": "#CBCCC6B3",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "source.ts punctuation.definition.typeparameters.begin",
        "source.ts punctuation.definition.typeparameters.end"
      ],
      "settings": {
        "foreground": "#FF9944",
        "fontStyle": ""
      }
    },
    // -------------- ðŸ”¥ SOURCE -----------------
    {
      "scope": ["source meta.block"],
      "settings": {
        "foreground": "#CBCCC6",
        "fontStyle": ""
      }
    },
    {
      "scope": ["source punctuation.separator.comma"],
      "settings": {
        "foreground": "#f29e7499",
        "fontStyle": ""
      }
    },
    {
      "scope": ["source punctuation.terminator", "source punctuation.separator.key-value"],
      "settings": {
        "foreground": "#CBCCC6B3",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "source punctuation.definition.template-expression.begin",
        "source punctuation.definition.template-expression.end"
      ],
      "settings": {
        "foreground": "#BAE67E",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "source punctuation.definition.string.template.begin.js",
        "source punctuation.definition.string.template.end.js",
        "source string.template punctuation.definition.string.template.begin",
        "source string.template punctuation.definition.string.template.end"
      ],
      "settings": {
        "foreground": "#FF9944",
        "fontStyle": ""
      }
    },
    {
      "scope": ["source new.expr entity.name.type"],
      "settings": { "foreground": "#F29E74", "fontStyle": "" }
    },
    {
      "scope": [
        "source string.template meta.template.expression meta.embedded.line variable.other.readwrite",
        "source string.template meta.template.expression meta.embedded.line variable.other"
      ],
      "settings": { "foreground": "#CBCCC6", "fontStyle": "" }
    },

    // -------------- ðŸ”¥ VUE -----------------
    // ---> console
    {
      "scope": [
        "source.vue meta.objectliteral meta.object.member meta.objectliteral meta.method.declaration meta.block meta.function-call entity.name.function.js",

        "source.vue meta.export.default meta.objectliteral meta.method.declaration meta.block meta.objectliteral meta.object.member meta.function-call entity.name.function.js",

        "source.vue meta.function-call entity.name.function.js"
      ],
      "settings": { "foreground": "#F27983", "fontStyle": "" }
    },
    {
      "scope": [
        "source.vue meta.block meta.function-call variable.other.object.js",
        "source.vue meta.function-call variable.other.object.js"
      ],
      "settings": { "foreground": "#59C9DE99", "fontStyle": "" }
    },
    // console <---
    // ---> function
    {
      "scope": [
        "source.vue meta.export.default meta.objectliteral meta.object.member meta.function-call entity.name.function.js"
      ],
      "settings": { "foreground": "#F27983", "fontStyle": "" }
    },
    {
      "scope": [
        "source.vue meta.export.default meta.objectliteral meta.object.member meta.objectliteral meta.method.declaration meta.block meta.function-call entity.name.function.js",

        "source.vue meta.export.default meta.objectliteral meta.object.member meta.method.declaration meta.block meta.block meta.function-call entity.name.function.js",

        "source.vue meta.export.default meta.objectliteral meta.object.member entity.name.function.js",

        "source.vue meta.export.default meta.objectliteral meta.object.member meta.objectliteral meta.object.member meta.function-call entity.name.function.js",

        "source.vue meta.export.default meta.objectliteral meta.method.declaration meta.block meta.var.expr meta.function-call entity.name.function.js",

        "source.vue meta.export.default meta.objectliteral meta.method.declaration meta.block meta.function-call entity.name.function.js"
      ],
      "settings": { "foreground": "#FFD580", "fontStyle": "" }
    },
    {
      "scope": ["source.vue punctuation.definition.tag.begin", "source.vue punctuation.definition.tag.end"],
      "settings": { "foreground": "#5CCFE6", "fontStyle": "" }
    },
    // function <---
    {
      "scope": ["source.vue text.html.vue-html meta.tag.block.any"],
      "settings": { "foreground": "#5CCFE6", "fontStyle": "" }
    },
    {
      "scope": [
        "source.vue meta.export.default meta.objectliteral meta.object.member meta.objectliteral meta.object.member meta.object-literal.key entity.name.function.js"
      ],
      "settings": { "foreground": "#FFD580", "fontStyle": "" }
    },
    {
      "scope": [
        "source.vue text.html.vue-html meta.tag.block.any.html meta.directive.vue source.directive.vue variable.other.readwrite.js"
      ],
      "settings": { "foreground": "#D4BFFF", "fontStyle": "" }
    },
    {
      "scope": ["source.vue text.html.vue-html expression.embedded.vue variable.other.readwrite.js"],
      "settings": { "foreground": "#D4BFFF", "fontStyle": "" }
