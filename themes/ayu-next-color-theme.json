  "semanticHighlighting": true,
  "semanticTokenColors": {
    "property.declaration": "#BAE67E",
    "readonly.local": "#BAE67E",
    // "variable": "#CBCCC6",
    // "variable.defaultLibrary": "#B0BEC5",
    // "variable.Default": "#5CCFE6",
    // "other": "#5CCFE6",
    "member": "#FFD580",
    // "*.readonly.local": "#FFFF8D",
    // "variable.local": "#FFFF8D",
    "function.local": "#CBCCC6",
    "variable.local": "#CBCCC6"
    // "entity.name": "#BCAAA4",
    // "entity.name": "#BCAAA4"
  },

  "tokenColors": [
    {
      "scope": ["support.function.console", "support.class.console"],
      "settings": { "foreground": "#0097A7", "fontStyle": "" }
    },
    {
      "scope": ["variable.parameter", "meta.parameter"],
      "settings": { "foreground": "#D4BFFFAE" }
    },
    {
      "scope": "comment",
      "settings": { "foreground": "#5c6773", "fontStyle": "italic" }
    },

    {
      "scope": ["variable.language.this"],
      "settings": { "foreground": "#5CCFE6", "fontStyle": "italic" }
    },
    {
      "scope": "variable.language.super",
      "settings": { "foreground": "#5CCFE6", "fontStyle": "bold" }
    },
    {
      "scope": "meta.object-literal.key",
      "settings": { "foreground": "#bae67e", "fontStyle": "" }
    },
    {
      "scope": "keyword.control.default",
      "settings": { "foreground": "#FFA759", "fontStyle": "bold" }
    },
    {
      "scope": "keyword.control.import",
      "settings": { "foreground": "#F29E74", "fontStyle": "" }
    },
    {
      "scope": "keyword.control.export",
      "settings": { "foreground": "#F29E74", "fontStyle": "" }
    },
    {
      "scope": "string, constant.other.symbol",
      "settings": { "foreground": "#bae67e", "fontStyle": "" }
    },
    {
      "scope": "string.regexp, constant.character, constant.other",
      "settings": { "foreground": "#95e6cb", "fontStyle": "" }
    },
    {
      "scope": "constant.numeric",
      "settings": { "foreground": "#8C9EFF", "fontStyle": "" }
    },
    {
      "scope": "constant.language",
      "settings": { "foreground": "#ffcc66", "fontStyle": "" }
    },
    {
      "scope": "meta.constant, entity.name.constant",
      "settings": { "foreground": "#d4bfff", "fontStyle": "" }
    },
    // {
    //   "scope": "variable",
    //   "settings": { "foreground": "#cbccc6", "fontStyle": "" }
    // },
    {
      "scope": "variable.member",
      "settings": { "foreground": "#f28779", "fontStyle": "" }
    },
    // {
    //   "scope": "variable.language",
    //   "settings": { "foreground": "#5ccfe6", "fontStyle": "italic" }
    // },
    {
      "scope": "storage, storage.type.keyword",
      "settings": { "foreground": "#ffa759", "fontStyle": "" }
    },
    {
      "scope": "keyword",
      "settings": { "foreground": "#ffa759", "fontStyle": "" }
    },
    {
      "scope": "keyword.operator",
      "settings": { "foreground": "#f29e74", "fontStyle": "" }
    },
    {
      "scope": "punctuation.separator, punctuation.terminator",
      "settings": { "foreground": "#cbccc6b3", "fontStyle": "" }
    },
    {
      "scope": "punctuation.section",
      "settings": { "foreground": "#cbccc6", "fontStyle": "" }
    },
    {
      "scope": "punctuation.accessor",
      "settings": { "foreground": "#f29e74", "fontStyle": "" }
    },

    {
      "scope": "entity.other.inherited-class",
      "settings": { "foreground": "#5ccfe6", "fontStyle": "" }
    },
    {
      "scope": "storage.type.function",
      "settings": { "foreground": "#ffa759", "fontStyle": "" }
    },

    // {

    //   "scope": "entity.name.function",
    //   "settings": { "foreground": "#ffd580", "fontStyle": "" }
    // },
    {
      "scope": "variable.parameter, meta.parameter",
      "settings": { "foreground": "#d4bfff", "fontStyle": "" }
    },
    {
      "scope": "variable.function, variable.annotation, meta.function-call.generic, support.function.go",
      "settings": { "foreground": "#ffd580", "fontStyle": "" }
    },
    // {

    //   "scope": "support.function, support.macro",
    //   "settings": { "foreground": "#f28779", "fontStyle": "" }
    // },
    {
      "scope": "entity.name.import, entity.name.package",
      "settings": { "foreground": "#bae67e", "fontStyle": "" }
    },
    // {

    //   "scope": "entity.name, source.js meta.function-call.constructor variable.type",
    //   "settings": { "foreground": "#73d0ff", "fontStyle": "" }
    // },
    {
      "scope": "entity.name.tag, meta.tag.sgml",
      "settings": { "foreground": "#5ccfe6", "fontStyle": "" }
    },
    {
      "scope": "punctuation.definition.tag.end, punctuation.definition.tag.begin, punctuation.definition.tag",
      "settings": { "foreground": "#5ccfe680", "fontStyle": "" }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": { "foreground": "#ffd580", "fontStyle": "" }
    },
    {
      "scope": "support.constant",
      "settings": { "foreground": "#f29e74", "fontStyle": "italic" }
    },
    // {

    //   "scope": "support.type, support.class, source.go storage.type",
    //   "settings": { "foreground": "#5ccfe6", "fontStyle": "" }
    // },
    {
      "scope": "meta.decorator variable.other, meta.decorator punctuation.decorator, storage.type.annotation, variable.annotation, punctuation.definition.annotation",
      "settings": { "foreground": "#ffe6b3", "fontStyle": "" }
    },
    {
      "scope": "invalid",
      "settings": { "foreground": "#ff3333", "fontStyle": "" }
    },
    {
      "scope": "meta.diff, meta.diff.header",
      "settings": { "foreground": "#c594c5", "fontStyle": "" }
    },
    {
      "scope": "source.ruby variable.other.readwrite",
      "settings": { "foreground": "#ffd580", "fontStyle": "" }
    },
    {
      "scope": "source.css entity.name.tag, source.sass entity.name.tag, source.scss entity.name.tag, source.less entity.name.tag, source.stylus entity.name.tag",
      "settings": { "foreground": "#73d0ff", "fontStyle": "" }
    },
    {
      "scope": "source.css support.type, source.sass support.type, source.scss support.type, source.less support.type, source.stylus support.type",
      "settings": { "foreground": "#5c6773", "fontStyle": "" }
    },
    {
      "scope": "support.type.property-name",
      "settings": { "foreground": "#5ccfe6", "fontStyle": "" }
    },
    {
      "scope": "constant.numeric.line-number.find-in-files - match",
      "settings": { "foreground": "#5c6773", "fontStyle": "" }
    },
    {
      "scope": "constant.numeric.line-number.match",
      "settings": { "foreground": "#ffa759", "fontStyle": "" }
    },
    {
      "scope": "entity.name.filename.find-in-files",
      "settings": { "foreground": "#bae67e", "fontStyle": "" }
    },
    {
      "scope": "message.error",
      "settings": { "foreground": "#ff3333", "fontStyle": "" }
    },
    {
      "scope": "markup.heading, markup.heading entity.name",
      "settings": { "foreground": "#bae67e", "fontStyle": "bold" }
    },
    {
      "scope": "markup.underline.link, string.other.link",
      "settings": { "foreground": "#5ccfe6", "fontStyle": "" }
    },
    {
      "scope": "markup.italic",
      "settings": { "foreground": "#f28779", "fontStyle": "italic" }
    },
    {
      "scope": "markup.bold",
      "settings": { "foreground": "#f28779", "fontStyle": "bold" }
    },
    {
      "scope": "markup.italic markup.bold, markup.bold markup.italic",
      "settings": { "foreground": "#f28779", "fontStyle": "bold italic" }
    },
    {
      "scope": "markup.raw",
      "settings": { "foreground": "#5c6773", "fontStyle": "bold italic" }
    },
    {
      "scope": "markup.raw.inline",
      "settings": { "foreground": "#5c6773", "fontStyle": "bold italic" }
    },
    {
      "scope": "meta.separator",

      "settings": { "foreground": "#5c6773", "fontStyle": "bold" }
    },
    {
      "scope": "markup.quote",
      "settings": { "foreground": "#95e6cb", "fontStyle": "italic" }
    },
    {
      "scope": "markup.list punctuation.definition.list.begin",
      "settings": { "foreground": "#ffd580", "fontStyle": "" }
    },
    {
      "scope": "markup.inserted",
      "settings": { "foreground": "#a6cc70", "fontStyle": "" }
    },
    {
      "scope": "markup.changed",
      "settings": { "foreground": "#77a8d9", "fontStyle": "" }
    },
    {
      "scope": "markup.deleted",
      "settings": { "foreground": "#f27983", "fontStyle": "" }
    },
    {
      "scope": "markup.strike",
      "settings": { "foreground": "#ffe6b3", "fontStyle": "" }
    },
    {
      "scope": "markup.table",

      "settings": { "foreground": "#5ccfe6", "fontStyle": "" }
